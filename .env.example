# Environment
NODE_ENV=development
PORT=3001
LOG_LEVEL=info

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/network_growth_engine

# Redis
REDIS_URL=redis://localhost:6379

# CORS
CORS_ORIGIN=http://localhost:3000

# LinkedIn Rate Limits
LINKEDIN_WEEKLY_LIMIT=100
LINKEDIN_DAILY_LIMIT=20
LINKEDIN_REQUEST_GAP_MIN=120
LINKEDIN_REQUEST_GAP_MAX=300
COOLDOWN_DAYS=7

# Feature Flags
GUIDED_MODE=true
LINKEDIN_AUTOMATION=false
GMAIL_INTEGRATION=false
CALENDAR_INTEGRATION=false

# Queue Settings
QUEUE_GENERATION_HOUR=7

# Google OAuth (Phase 3)
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=
